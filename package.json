{
  "name": "conferences",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.39",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "crypto-js": "^4.1.1",
    "get-google-fonts": "^1.2.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-scripts": "5.0.1",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "ts-jest": "^29.1.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --watch",
    "coverage": "start coverage/lcov-report/index.html",
    "eject": "react-scripts eject",
    "lint-error": "eslint src/**/*.ts src/**/*.tsx . --ext .ts --max-warnings 0",
    "lint-fix": "eslint --fix --ext .ts,.tsx .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,css,scss}\"",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.2",
    "@types/react-slick": "^0.23.10",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "eslint-plugin-jest": "^27.2.3",
    "formik": "^2.4.3",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "js-cookie": "^3.0.5",
    "prettier": "^3.0.1",
    "react-router-dom": "^6.14.2",
    "react-toastify": "^9.1.3",
    "yup": "^1.2.0"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "npx prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,css,scss}\"",
      "npx eslint --fix --ext .ts,.tsx .",
      "npx eslint src/**/*.ts src/**/*.tsx . --ext .ts --max-warnings 0"
    ]
  }
}
